package xupt.se.ttms.model;

import java.sql.Date;

public class Ticket {

	private int ticket_id;//票id
	private int seat_id; //座位id
	private int sched_id; //演出计划id
    private float ticket_price;//票价
	private Date ticket_times;//上印时间
	private int ticket_status;//0:代销售,1:锁定,2:卖出.

	public int getTicket_id() {
		return ticket_id;
	}

	public void setTicket_id(int ticket_id) {
		this.ticket_id = ticket_id;
	}

	public int getSeat_id() {
		return seat_id;
	}

	public void setSeat_id(int seat_id) {
		this.seat_id = seat_id;
	}

	public int getSched_id() {
		return sched_id;
	}

	public void setSched_id(int sched_id) {
		this.sched_id = sched_id;
	}

	public float getTicket_price() {
		return ticket_price;
	}

	public void setTicket_price(float ticket_price) {
		this.ticket_price = ticket_price;
	}

	public Date getTicket_times() {
		return ticket_times;
	}

	public void setTicket_times(Date ticket_times) {
		this.ticket_times = ticket_times;
	}

	public int getTicket_status() {
		return ticket_status;
	}

	public void setTicket_status(int ticket_status) {
		this.ticket_status = ticket_status;
	}
}
